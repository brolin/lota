#nav
  display: inline-block
  li
    float: left
    position: relative
    list-style: none

/* main level link */
#nav a
  corner(6px)
  background: verde-obscuro
  display: inline-block
  padding: 10px
  float: left
  color: white
  text-decoration: none
  &:hover
    background-color: white
    color: verde-obscuro

/* main level link hover */
#nav .current a, #nav li:hover > a
  corner(6px)
  background: verde-obscuro
  color: white
  text-decoration: none
  &:hover
    background-color: white
    color: verde-obscuro

/* sub levels link hover */
#nav ul li:hover a, #nav li:hover li a
  corner(6px)
  background: verde-obscuro
  color: white
  text-decoration: none

#nav ul a:hover
  corner(6px)
  background: verde-obscuro
  display: inline-block
  color: white
  text-decoration: none

#nav ul
  display: none
  position: absolute
  top: 35px
  left: 0

#nav li:hover > ul
  corner(6px)
  background: verde-obscuro
  display: inline-block
  color: white
  text-decoration: none

#nav ul li
  corner(6px)
  background: verde-obscuro
  display: inline-block
  color: white
  text-decoration: none

#nav ul a
  corner(6px)
  background: verde-obscuro
  display: inline-block
  float: left
  color: white
  text-decoration: none

#nav:after
  content: "."
  display: block
  clear: both
  visibility: hidden
  line-height: 0
  height: 0

