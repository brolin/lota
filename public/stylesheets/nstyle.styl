////////////////////////////////////////////////
//                   UTILS    
////////////////////////////////////////////////

corner-each(tl, tr, br, bl)
  border-radius tl
  -moz-border-radius-topleft tl
  -webkit-border-top-left-radius tl
  -moz-border-radius-topright tr
  -webkit-border-top-right-radius tr
  -moz-border-radius-bottomleft bl
  -webkit-border-bottom-left-radius bl
  -moz-border-radius-bottomright br
  -webkit-border-bottom-right-radius br

corner(px)
  corner-each(px,px,px,px)

corner-top(px)
  corner-each(px,px,0,0)

corner-bottom(px)
  corner-each(0,0,px,px)

corner-left(px)
  corner-each(px,0,0,px)

corner-right(px)
  corner-each(0,px,px,0)

bx-shadow(shw-size, shw-color)
  -moz-box-shadow: 0px 0px shw-size shw-color
  -webkit-box-shadow: 0px 0px shw-size shw-color
  box-shadow: 0px 0px shw-size shw-color

txt-shadow(shw-size, shw-color)
  -moz-text-shadow: 0px 0px shw-size shw-color
  -webkit-tex-shadow: 0px 0px shw-size shw-color
  text-shadow: 0px 0px shw-size shw-color


bg-gradient(color1,color2)
  background-image: -moz-linear-gradient(top, color1, color2)
  background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0, color1),color-stop(1, color2))
  background-image: -webkit-linear-gradient(top, color1, color2)
  background-image: -moz-linear-gradient(top, color1, color2)
  background-image: -ms-linear-gradient(top, color1, color2)
  background-image: -o-linear-gradient(top, color1, color2)

